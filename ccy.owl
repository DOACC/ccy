        <skos:prefLabel xml:lang="en">PKO-can-encrypt</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:Class>

    <owl:Class rdf:about="&ccy;SigningPublicKeyObject">
        <dc:description xml:lang="en">PublicKeyObjects which are capable of signing things. True if the algorithm is capable of signing data; false otherwise. To test if a given key object can sign data, use CanSign and HasPrivate.</dc:description>
