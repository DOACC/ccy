<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/stylesheets/assets/ns-schema.xsl"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY ccy "http://purl.org/net/bel-epa/ccy#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY ns "http://www.w3.org/2003/06/sw-vocab-status/ns#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY doacc "http://purl.org/net/bel-epa/doacc" >
]>
<rdf:RDF xmlns="http://purl.org/net/bel-epa/ccy#"
     xml:base="http://purl.org/net/bel-epa/ccy"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:ccy="http://purl.org/net/bel-epa/ccy#"
     xmlns:doacc="http://purl.org/net/bel-epa/doacc">
    <owl:Ontology rdf:about="&ccy;">
        <terms:created>2014-05-22</terms:created>
        <rdfs:comment xml:lang="en">Ontology for a cryptocurrency.</rdfs:comment>
        <rdfs:label>Ontology for a cryptocurrency, based on Melvyn Carvalho’s “Ontology for Crypto Currencies” with a few adjustments and additions.</rdfs:label>
        <ns:term_status>unstable</ns:term_status>
        <foaf:maker rdf:resource="http://bel-epa.com/gjh#gjh"/>
        <owl:imports rdf:resource="http://www.w3.org/TR/skos-reference/skos-owl1-dl.rdf"/>
        <owl:imports rdf:resource="https://raw.githubusercontent.com/DOACC/doacc/master/doacc.owl"/>
        <owl:versionIRI rdf:resource="http://purl.org/net/bel-epa/ccy/1.0"/>
    </owl:Ontology>

    <owl:Class rdf:about="&ccy;ElectronicCoin">
        <rdfs:comment xml:lang="en">We define an Electronic coin as a chain of digital signatures.  As per Satoshi's paper.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">ElectronicCoin</rdfs:label>
        <skos:prefLabel xml:lang="en">ElectronicCoin</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;PublicKeyObject">
        <rdfs:comment xml:lang="en">An object corresponding to a key for some algorithm. The object can hold a public and optionally a private key.</rdfs:comment>
        <skos:prefLabel xml:lang="en">Public-Key-Object</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:Class>

    <owl:Class rdf:about="&ccy;EncryptingPublicKeyObject">
        <rdfs:comment xml:lang="en">PublicKeyObjects which are capable of encrypting things</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&ccy;PublicKeyObject"/>
        <skos:prefLabel xml:lang="en">PKO-can-encrypt</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:Class>

    <owl:Class rdf:about="&ccy;SigningPublicKeyObject">
        <rdfs:comment xml:lang="en">PublicKeyObjects which are capable of signing things. True if the algorithm is capable of signing data; false otherwise. To test if a given key object can sign data, use CanSign and HasPrivate.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&ccy;PublicKeyObject"/>
        <skos:prefLabel xml:lang="en">PKO-can-sign</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:Class>

    <owl:Class rdf:about="&ccy;PublicKeyObjectWithPrivateKey">
        <rdfs:comment xml:lang="en">Some keys have private parts, some don't.  This is the Class of those which do.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="&ccy;PublicKeyObject"/>
        <skos:prefLabel xml:lang="en">PKO-has-private</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:Class>

    <owl:Class rdf:about="&ccy;HashFunction">
        <rdfs:comment xml:lang="en">Crypographic hash function.</rdfs:comment>
        <rdfs:label>A crypographic hash function</rdfs:label>
        <skos:prefLabel xml:lang="en">HashFunction</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:Class>

    <owl:Class rdf:about="&ccy;BlockChain">
        <rdfs:comment xml:lang="en">A </rdfs:comment>
        <rdfs:label>A blockchain used by an electronic coin</rdfs:label>
        <skos:prefLabel xml:lang="en">Blockchain</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:Class>

    <owl:Class rdf:about="&ccy;Block">
        <rdfs:comment xml:lang="en">A Block for a crypto currency</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">Block</rdfs:label>
        <skos:prefLabel xml:lang="en">Block</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;BlockHeader">
        <rdfs:comment xml:lang="en">A Block Header for a crypto currency</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">BlockHeader</rdfs:label>
        <skos:prefLabel xml:lang="en">BlockHeader</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;MerkleRoot">
        <rdfs:comment xml:lang="en">The root of a Merkle tree, of items. Merkle trees are binary trees of hashes.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">MerkleRoot</rdfs:label>
        <skos:prefLabel xml:lang="en">MerkleRoot</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;DigitalSignature">
        <rdfs:comment xml:lang="en">An Elliptic Curve Digital Signature Algorithm (ECDSA) signature.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">DigitalSignature</rdfs:label>
        <skos:prefLabel xml:lang="en">DigitalSignature</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;Transaction">
        <rdfs:comment xml:lang="en">A Block Transaction for a crypto currency</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">Transaction</rdfs:label>
        <skos:prefLabel xml:lang="en">Transaction</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;TransactionInput">
        <rdfs:comment xml:lang="en">A Block Transaction Input for a crypto currency. An array of objects with each object being an input vector (vin) for this transaction. Input objects will have the same order within the array as they have in the transaction, so the first input listed will be input 0</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">TransactionInput</rdfs:label>
        <skos:prefLabel xml:lang="en">TransactionInput</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;TransactionOutput">
        <rdfs:comment xml:lang="en">A Block Transaction Output for a crypto currency</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">TransactionOutput</rdfs:label>
        <skos:prefLabel xml:lang="en">TransactionOutput</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;Parameter">
        <rdfs:comment xml:lang="en">A parameter variable for a crypto currency</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">Parameter</rdfs:label>
        <skos:prefLabel xml:lang="en">Parameter</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;VOut">
        <rdfs:comment xml:lang="en">Sequence of output txs</rdfs:comment>
        <rdfs:type rdf:resource="&rdfs;List"/>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">VOut</rdfs:label>
        <skos:prefLabel xml:lang="en">VOut</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:Class rdf:about="&ccy;VIn">
        <rdfs:comment xml:lang="en">Sequence of input txs</rdfs:comment>
        <rdfs:type rdf:resource="&rdfs;List"/>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">VIn</rdfs:label>
        <skos:prefLabel xml:lang="en">VIn</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:Class>

    <owl:ObjectProperty rdf:about="&ccy;blockchain">
        <rdfs:comment xml:lang="en">A blockchain generated by the coin</rdfs:comment>
        <rdfs:domain rdf:resource="&ccy;ElectronicCoin"/>
        <rdfs:range rdf:resource="&ccy;BlockChain"/>
        <skos:prefLabel xml:lang="en">blockchain</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;linkin">
        <rdfs:comment xml:lang="en">A link in a blockchain</rdfs:comment>
        <rdfs:domain rdf:resource="&ccy;BlockChain"/>
        <rdfs:range rdf:resource="&ccy;Block"/>
        <skos:prefLabel xml:lang="en">linkin</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;publicKey">
        <rdfs:comment xml:lang="en">The object is a public key object that doesn't contain the private key data in the subject. This function extracts the public part.</rdfs:comment>
        <rdfs:domain rdf:resource="&ccy;PublicKeyObject"/>
        <rdfs:range rdf:resource="&ccy;PublicKeyObject"/>
        <skos:prefLabel xml:lang="en">publicKey</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;previousBlockHeader">
        <rdfs:comment xml:lang="en">The previous block header</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">previousBlockHeader</rdfs:label>
        <skos:prefLabel xml:lang="en">previousBlockHeader</skos:prefLabel>
        <rdfs:domain rdf:resource="&ccy;Block"/>
        <rdfs:range rdf:resource="&ccy;BlockHeader"/>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;protocolSpecification">
        <rdfs:comment xml:lang="en">The protocol used by an electronic coin.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">protocolSpecification</rdfs:label>
        <skos:prefLabel xml:lang="en">protocolSpecification</skos:prefLabel>
        <rdfs:domain rdf:resource="&ccy;ElectronicCoin"/>
        <rdfs:range rdf:resource="http://purl.org/net/bel-epa/doacc#Protocol"/>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;transactionMerkleRoot">
        <rdfs:comment xml:lang="en">The hash of the merkle tree of transactions.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">transactionMerkleRoot</rdfs:label>
        <rdfs:domain rdf:resource="&ccy;Transaction"/>
        <rdfs:range rdf:resource="&ccy;MerkleRoot"/>
        <skos:prefLabel xml:lang="en">transactionMerkleRoot</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;nbits">
        <rdfs:comment xml:lang="en">This is the term used in the reference client.  It is the target, but in a compact and custom 4-byte encoding.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">nbits</rdfs:label>
        <rdfs:domain rdf:resource="&ccy;ElectronicCoin"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <skos:prefLabel xml:lang="en">nbits</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;scriptSig">
        <rdfs:comment xml:lang="en">The input scriptSig, the signature script of this input. Not present if this is a coinbase transaction</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">scriptSig</rdfs:label>
        <skos:prefLabel xml:lang="en">scriptSig</skos:prefLabel>
        <rdfs:domain rdf:resource="&ccy;TransactionInput"/>
        <rdfs:domain rdf:resource="&ccy;DigitalSignature"/>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;sequence">
        <rdfs:comment xml:lang="en">Transaction sequence.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">sequence</rdfs:label>
        <skos:prefLabel xml:lang="en">sequence</skos:prefLabel>
        <rdfs:domain rdf:resource="&ccy;Block"/>
        <rdfs:range rdf:resource="&rdfs;List"/>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;testnet3">
        <rdfs:comment xml:lang="en">This represents a coin testnet3 address which should be a coin URI, e.g. bitcoin:</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">testnet3</rdfs:label>
        <skos:prefLabel xml:lang="en">testnet3</skos:prefLabel>
        <rdfs:domain rdf:resource="&ccy;ElectronicCoin"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;timestamp">
        <rdfs:comment xml:lang="en">A unix timestamp for crypto blocks</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">timestamp</rdfs:label>
        <skos:prefLabel xml:lang="en">timestamp</skos:prefLabel>
        <rdfs:domain rdf:resource="&ccy;Block"/>
        <rdfs:range rdf:resource="&xsd;dateTimeStamp"/>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;hasTransaction">
        <rdfs:comment xml:lang="en">The block has a transaction.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:domain rdf:resource="&ccy;Block"/>
        <rdfs:range rdf:resource="&ccy;Transaction"/>
        <rdfs:label xml:lang="en">hasTransaction</rdfs:label>
        <skos:prefLabel xml:lang="en">hasTransaction</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;height">
        <rdfs:comment xml:lang="en">The height according to the longest chain.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">height</rdfs:label>
        <rdfs:domain rdf:resource="&ccy;ElectronicCoin"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <skos:prefLabel xml:lang="en">height</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;index">
        <rdfs:comment xml:lang="en">The index number of an input or output, sometimes referred to as vout or n</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">index</rdfs:label>
        <skos:prefLabel xml:lang="en">index</skos:prefLabel>
        <rdfs:domain rdf:resource="&ccy;Transaction"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;latestBlockHeader">
        <rdfs:comment xml:lang="en">The latest block header for an electronic coin.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">latestBlockHeader</rdfs:label>
        <skos:prefLabel xml:lang="en">latestBlockHeader</skos:prefLabel>
        <rdfs:domain rdf:resource="&ccy;Block"/>
        <rdfs:range rdf:resource="&ccy;BlockHeader"/>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;coinURI">
        <rdfs:domain rdf:resource="&ccy;ElectronicCoin"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:comment xml:lang="en">This represents a coin address which should be a coin URI e.g. bitcoin: </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">coinURI</rdfs:label>
        <skos:prefLabel xml:lang="en">coinURI</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="&ccy;md5">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The object is a MD5 hash of the subject.</rdfs:comment>
        <rdfs:domain rdf:resource="&rdfs;Literal"/>
        <rdfs:label>MD5 hash</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <skos:prefLabel xml:lang="en">md5</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&ccy;sha">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">The object is a SHA-1 hash of the subject.</rdfs:comment>
        <rdfs:domain rdf:resource="&rdfs;Literal"/>
        <rdfs:label>SHA hash</rdfs:label>
        <skos:prefLabel xml:lang="en">SHA-1</skos:prefLabel>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&ccy;sign">
        <rdfs:comment xml:lang="en">The subject should be a list of two things, a hash string and a key (containing private and public parts). The object is calculated as a signature string by signing the hash with the key's private part.</rdfs:comment>
        <rdfs:domain rdf:resource="&ccy;PublicKeyObject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>sign</rdfs:label>
        <skos:prefLabel xml:lang="en">sign</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&ccy;verify">
        <rdfs:comment xml:lang="en">If the subject is a key object containing private and public parts and the obejct is a list of a hash and a signature, then this is true if and only if the signature is a valid signature of the hash with the key.</rdfs:comment>
        <rdfs:domain rdf:resource="&ccy;PublicKeyObject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <skos:prefLabel xml:lang="en">verify</skos:prefLabel>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&ccy;verifyBoolean">
        <skos:prefLabel xml:lang="en">verifyBoolean</skos:prefLabel>
        <rdfs:domain rdf:resource="&ccy;PublicKeyObject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">If the subject is a list containg a keypair, a hash, and a signature, then the object is either "1" if the signature validates or "0" if it does not.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
    </owl:DatatypeProperty>

    <owl:ObjectProperty rdf:about="&ccy;hasInput">
        <rdfs:comment xml:lang="en">A block transaction has an input.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">hasInput</rdfs:label>
        <rdfs:domain rdf:resource="&ccy;Transaction"/>
        <rdfs:range rdf:resource="&ccy;TransactionInput"/>
        <skos:prefLabel xml:lang="en">hasInput</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&ccy;hasOutput">
        <rdfs:comment xml:lang="en">A block transaction has an ouput.</rdfs:comment>
        <rdfs:domain rdf:resource="&ccy;Transaction"/>
        <rdfs:range rdf:resource="&ccy;TransactionOutput"/>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">hasOutput</rdfs:label>
        <skos:prefLabel xml:lang="en">hasOutput</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:ObjectProperty>

    <!--
    <owl:DatatypeProperty rdf:about="&ccy;blockhash">
        <rdfs:comment xml:lang="en">A hash of a block</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label xml:lang="en">blockhash</rdfs:label>
        <skos:prefLabel xml:lang="en">blockhash</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:DatatypeProperty>
    -->

    <owl:DatatypeProperty rdf:about="&ccy;blockhash">
        <rdfs:comment xml:lang="en">If the transaction has been included in a block on the local best block chain, this is the hash of that block encoded as hex in RPC byte order</rdfs:comment>
        <rdfs:domain rdf:resource="&ccy;scriptSig"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">blockhash</rdfs:label>
        <skos:prefLabel xml:lang="en">blockhash</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&ccy;blocktime">
        <rdfs:comment xml:lang="en">This field is currently identical to &ccy;time</rdfs:comment>
        <rdfs:domain rdf:resource="&ccy;scriptSig"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:isDefinedBy rdf:resource="&ccy;"/>
        <rdfs:label xml:lang="en">blocktime</rdfs:label>
        <skos:prefLabel xml:lang="en">blocktime</skos:prefLabel>
        <ns:term_status>unstable</ns:term_status>
